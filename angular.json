{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Omnivus": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Omnivus",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/all.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/nice-select.css",
              "src/assets/css/animate.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/assets/css/slick.css",
              "src/assets/css/default.css",
              "src/assets/css/style.css",
              "src/styles.css",
              "src/assets/css/general.css"
            ],
            "scripts": [
              "src/assets/js/vendor/modernizr-3.6.0.min.js",
              "src/assets/js/vendor/jquery-1.12.4.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/js/imagesloaded.pkgd.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/circle-progress.min.js",
              "src/assets/js/ajax-contact.js",
              "src/assets/js/jquery.syotimer.min.js",
              "src/assets/js/jquery.nice-select.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Omnivus:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Omnivus:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Omnivus:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Omnivus:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Omnivus:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "Omnivus",
  "cli": {
    "analytics": false
  }
}
